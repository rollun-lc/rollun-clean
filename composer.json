{
    "name": "rollun-com/rollun-clean",
    "description": "Base classes for clean architecture",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yurii Orlyk",
            "email": "orlyk.rollun@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "php": ">=8.0",
        "phpunit/phpunit": "^9.5.10",
        "rollun-com/rollun-callback": "^7.0.0",
        "rollun-com/rollun-datastore": "^8.0.1",
        "rollun-com/rollun-logger": "^7.0.0",
        "rollun-com/rollun-utils": "^7.0.0",
        "laminas/laminas-component-installer": "^2.6",
        "laminas/laminas-config-aggregator": "^1.7",
        "laminas/laminas-diactoros": "^2.8",
        "laminas/laminas-servicemanager": "^3.10",
        "laminas/laminas-stdlib": "^3.7.0",
        "mezzio/mezzio": "^3.9",
        "mezzio/mezzio-fastroute": "^3.4",
        "mezzio/mezzio-helpers": "^5.8",
        "symfony/dotenv": "^6.0.3",
        "articus/data-transfer": "^0.5.2",
        "rollun-com/rollun-openapi": "^10.0.0",
        "guzzle/guzzle": "dev-php-8.0 as v3.9.0",
        "symfony/serializer": "^6.0",
        "symfony/property-info": "^v6.0.14",
        "symfony/property-access": "^v6.0.11"
    },
    "suggest": {
        "rollun-com/rollun-datastore": "^7.0.0",
        "rollun-com/rollun-utils": "^5.1.1"
    },
    "autoload": {
        "psr-4": {
            "Clean\\Common\\": "src",
            "example\\": "example/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\": "test",
            "example\\": "example"
        }
    },
    "config": {
        "allow-plugins": {
            "zendframework/zend-component-installer": true,
            "rollun-com/rollun-installer": true,
            "laminas/laminas-component-installer": true
        }
    },
    "repositories": [
        {
            "type": "github",
            "url": "git@github.com:orlyk-rollun/guzzle3.git"
        }
    ]
}
